<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Index</title>
    </head>

    <body>
        <script>
            var settings = `<%- settings %>`;
            try {
                window.SETTINGS = JSON.parse(settings);
                console.log(SETTINGS);
            } catch (error) {
                window.SETTINGS = null;
                console.log(error);
            }
        </script>
    </body>
</html>
